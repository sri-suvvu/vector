#include<bits/stdc++.h>

using namespace std;
int main()
{
	vector<int>ve;
	ve.push_back(10);
	ve.push_back(20);
	ve.emplace_back(30);
	for(int i=0;i<ve.size();i++)
		cout<<ve[i];
	cout<<"\n";
	ve.pop_back();
	for(auto it:ve)
		cout<<it<<" ";
	cout<<"\n";
	vector<int>vet;
	vet.push_back(48);
	vet.push_back(50);
	vet.push_back(7);
	vet.emplace_back(43);
	//way to print the vector
	for(vector<int>::iterator it=vet.begin();it!=vet.end();it++)
           {
           	cout<<*(it)<<" ";
           }
           cout<<"\n";
           //reverse
    for(auto it=vet.rbegin();it!=vet.rend();it++)
           {
           	cout<<*(it)<<" ";
          }
          cout<<endl;
      }
